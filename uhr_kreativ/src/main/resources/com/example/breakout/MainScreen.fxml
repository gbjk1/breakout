     <Button fx:id="ButtonStart" layoutX="558.0" layoutY="276.0" mnemonicParsing="false" onAction="#switchToLevels" onMouseEntered="#changePictureMainScreen" onMouseExited="#changePictureMainScreen" opacity="0.0" prefHeight="52.0" prefWidth="175.0" />
