     <Button fx:id="ButtonSecret" layoutX="1.0" layoutY="668.0" mnemonicParsing="false" onAction="#showClockScreen" onMouseEntered="#changePictureMainScreen" onMouseExited="#changePictureMainScreen" opacity="0.0" prefHeight="52.0" prefWidth="155.0" />
